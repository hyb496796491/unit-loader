    }
  },
  "keywords": [
    "webpack",
    "loader",
    "unit"
  ],
  "author": "superTerrorist",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/superTerrorist/unit-loader/issues"
  },
  "homepage": "https://github.com/superTerrorist/unit-loader#readme",
  "dependencies": {
    "postcss": "^7.0.5"
  },
  "devDependencies": {
    "husky": "^1.1.4",
    "prettier": "^1.15.2",
    "standard-version": "^4.4.0"
  }
}
