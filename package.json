{
  "name": "unit-loader",
  "version": "0.1.1",
  "description": "unit transform loader for webpack ",
  "main": "index.js",
  "scripts": {
    "prettier": "prettier --write 'src/**/*.js'",
    "build": "node script/build.js",
    "release": "standard-version",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/superTerrorist/unit-loader.git"
